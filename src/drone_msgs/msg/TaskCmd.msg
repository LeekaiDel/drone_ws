# Типы команд задания
byte TAKEOFF = 0                            # Взлет (При взлете заполняем поле coordinates одной точкой взлета)
byte GO_TO_POINT = 1                        # Достигнуть точки  (При команде достигнуть точки заполняем поле coordinates одной целевой точкой)
byte GO_ALONG_TRAJECTORY = 2                # Движение по траектории    (При команде движение по траектории, заполняем поле coordinates последовательностью целевых точек)
byte STOP = 4                               # Остановиться в текущей позиции    (При команде стоп, поле coordinates остается пустым)
byte RETURN = 5                             # Возврат в начальную позицию   (При команде возврат в начальную позицию, поле coordinates остается пустым)
byte LAND = 6                               # Посадка   (При команде посадка, поле coordinates остается пустым)

std_msgs/Header header
byte type                                   # Тип команды   (Передаем сюда один из типов команды из перечня выше)
int8 priority                               # Приоритет команды  (Чем выше значение тем выше приоритет команды)
geometry_msgs/PoseStamped[] coordinates     # Набор координат